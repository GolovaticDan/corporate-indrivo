{
    "name": "indrivo-profile/corporate-profile",
    "description": "Project template for Corporate Indrivo with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.indrivo.com",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "indrivo/corporate-indrivo",
                "version": "dev-main",
                "type": "drupal-profile",
                "source": {
                    "url": "https://deploy:wxF56VUGsFr7v-y7P6zx@gitlab.dev.indrivo.com/php-team/corporate-indrivo.git",
                    "type": "git",
                    "reference": "1.0"
                },
                "minimum-stability": "dev",
                "require": {
                    "drupal/core": "^8 || ^9 || ^10"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "indrivo/web_feedback",
                "version": "dev-develop",
                "type": "drupal-module",
                "source": {
                    "url": "https://deploy:8XXuhFsVYjSgLtutypuh@gitlab.dev.indrivo.com/php-team/module_web_feedback.git",
                    "type": "git",
                    "reference": "develop"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "indrivo/drupaltograylog",
                "version": "1.0",
                "tag": "1.0.0",
                "type": "drupal-module",
                "source": {
                    "url": "https://deploy:cArMDppXv4j7n73P2ZcA@gitlab.dev.indrivo.com/php-team/drupaltograylog.git",
                    "type": "git",
                    "reference": "1.0.0"
                },
                "require": {
                    "graylog2/gelf-php": "^1.7"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.3",
        "drupal/advagg": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/checklistapi": "^2.1",
        "drupal/colorbox": "^2.0",
        "drupal/config_update": "^1.7",
        "drupal/cookies": "^1.2",
        "drupal/ctools": "^3.13",
        "drupal/devel": "^5.0",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/elogger": "^1.1",
        "drupal/features": "^3.12",
        "drupal/fixed_block_content": "^1.1",
        "drupal/google_analytics": "^4.0",
        "drupal/lazy": "^3.11",
        "drupal/metatag": "^1.22",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/realistic_dummy_content": "^3.1",
        "drupal/recaptcha_v3": "^1.7",
        "drupal/recreate_block_content": "^2.1",
        "drupal/simple_sitemap": "^4.1",
        "drupal/social_auth_facebook": "^3.0",
        "drupal/social_auth_google": "^3.0",
        "drupal/social_auth_linkedin": "^3.1",
        "drupal/svg_image": "^1.16",
        "indrivo/web_feedback": "dev-develop",
        "drupal/yoast_seo": "^1.8",
        "drush/drush": "^11.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "web/profiles/contrib/corporate_indrivo/modules/custom/*/composer.json"
            ],
            "recurse": true
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
